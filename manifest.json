{
  "manifest_version": 2,
  "name": "Telegram extension",
  "version": "1.1",
  "icons": {
    "16": "app/icon.png",
    "48": "app/icon.png",
    "128": "app/icon.png"
  },
  "browser_action": {
    "default_icon": "app/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://web.telegram.org/*"
      ],
      "css": ["app/css/fullScreen.css", "app/css/darkTheme.css"]
    }
  ]
}